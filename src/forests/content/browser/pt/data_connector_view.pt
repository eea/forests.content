<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  lang="en"
  metal:use-macro="context/@@main_template/macros/master"
  i18n:domain="plone">

  <body>
    <metal:main fill-slot="main">
      <h1 class="documentFirstHeading" tal:content="context/Title">Title</h1>

      <div tal:content="context/query">Query definition here</div>

      <table class="table listing" tal:define="data view/data">
        <tr tal:repeat="row data">
          <td tal:repeat="cell row" tal:content="cell">Cell value</td>
        </tr>
      </table>

    </metal:main>
  </body>
</html>
